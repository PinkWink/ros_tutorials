<?xml version="1.0" ?>
<robot name="mobile_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find mobile_robot_description)/urdf/gazebo.xacro.urdf"/>

    <link name="base_footprint"/>
    <link name="base_link">
        <visual>
            <origin xyz="-0.08 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.26 0.1"/>
            </geometry>
            <material name="gray">
                <color rgba="0.5 0.5 0.5 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="-0.08 0 0.03" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.26 0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="2.0"/>
            <inertia ixx="0.0042" ixy="0.0" ixz="0.0" iyy="0.0032" iyz="0.0" izz="0.0066"/>
        </inertial>
    </link>

    <link name="l_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.570796 0 0"/>
            <geometry>
                <cylinder radius="0.035" length="0.03"/>
            </geometry>
            <material name="dark_gray">
                <color rgba="0.2 0.2 0.2 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.570796 0 0"/>
            <geometry>
                <cylinder radius="0.035" length="0.03"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.0002" ixy="0.0" ixz="0.0" iyy="0.0002" iyz="0.0" izz="0.0003"/>
        </inertial>
    </link>

    <link name="r_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.570796 0 0"/>
            <geometry>
                <cylinder radius="0.035" length="0.03"/>
            </geometry>
            <material name="dark_gray">
                <color rgba="0.2 0.2 0.2 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.570796 0 0"/>
            <geometry>
                <cylinder radius="0.035" length="0.03"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.0002" ixy="0.0" ixz="0.0" iyy="0.0002" iyz="0.0" izz="0.0003"/>
        </inertial>
    </link>

    <link name="caster_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.02" />
            </geometry>
            <material name="light_gray">
                <color rgba="0.8 0.8 0.8 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.02" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <inertia ixx="0.00013" ixy="0.0" ixz="0.0" iyy="0.00013" iyz="0.0" izz="0.00013"/>
        </inertial>
    </link>

    <joint name="base_footprint_fixed" type="fixed">
        <origin xyz="0 0 0.035" rpy="0 0 0" />
        <parent link="base_footprint"/>
        <child link="base_link"/>
    </joint>

    <joint name="l_wheel_joint" type="continuous">
        <origin xyz="0 0.145 0" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="l_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="r_wheel_joint" type="continuous">
        <origin xyz="0 -0.145 0" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="r_wheel"/>
        <axis xyz="0 1 0"/>
    </joint>

    <joint name="caster_wheel_joint" type="fixed">
        <origin xyz="-0.190 0 -0.015" rpy="0 0 0" />
        <parent link="base_link"/>
        <child link="caster_wheel"/>
    </joint>
</robot>